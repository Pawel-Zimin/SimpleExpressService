extends ../layout

block content
  h1= title
  nav
    ul
      li
        a(href='/admin/news/add') Dodaj
  table
    tr
      th id
      th tytuł
      th data dodania
      th akcja
    each el, index in data
      tr
        td=index
        td=el.title
        td=el.created
        td
          a(href=`/admin/news/delete/${el._id}`) Usuń
